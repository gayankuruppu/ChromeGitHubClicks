{
  "name": "Git Clicks",
  "version": "1.0",
  "description": "Navigate through the class names and declarations in Github like an IDE",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["js/vendor/jquery-3.4.1.js", "js/content.js", "js/github.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2
}
